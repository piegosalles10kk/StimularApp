{
  "expo": {
    "name": "StimularApp",
    "slug": "Stimular",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./imagensIcons/icon.png",  
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./imagensIcons/icon.png",  
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./imagensIcons/icon.png", 
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "package": "com.piegosalles.Stimular"
    },
    "web": {
      "favicon": "./imagensIcons/icon.png"  
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "7b061c37-1511-476e-ae12-f6aa8bdedaab"
      }
    }
  }
}
